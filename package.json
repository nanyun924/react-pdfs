{
  "name": "react-pdf",
  "version": "0.1.0",
  "description": "A tiny React renderer to generate PDF documents.",
  "main": "./index.js",
  "scripts": {
    "example": "babel-node ./examples/example.jsx",
    "build": "babel ./src --out-dir ./lib",
    "build:watch": "npm run build -- --watch",
    "precommit": "lint-staged",
    "lint": "eslint performance src --ignore-path .gitignore"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint",
      "prettier --single-quote --trailing-comma=all --write",
      "git add"
    ]
  },
  "keywords": [
    "react",
    "reactjs",
    "renderer",
    "pdf"
  ],
  "author": "Diego Muracciole <diegomuracciole@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.6.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.6.0",
    "babel-eslint": "^7.2.1",
    "eslint": "^3.18.0",
    "eslint-config-prettier": "^1.5.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^6.10.3",
    "husky": "^0.13.2",
    "lint-staged": "^3.4.0",
    "prettier": "^0.22.0"
  },
  "dependencies": {
    "fbjs": "^0.8.4",
    "pdfkit": "*",
    "react": "16.0.0-alpha",
    "react-dom": "16.0.0-alpha"
  },
  "homepage": "https://github.com/diegomura/react-pdf#readme",
  "repository": "git@github.com:diegomura/react-pdf.git"
}
